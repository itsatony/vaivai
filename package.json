{
  "name": "vaivai",
  "displayName": "Vaivai",
  "description": "A VSCode extension for vaudience.ai to improve developer productivity",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vaivai.generateFileList",
        "title": "Vaivai: Generate File List"
      },
      {
        "command": "vaivai.generateContentOverview",
        "title": "Vaivai: Generate Content Overview"
      },
      {
        "command": "vaivai.viewErrorLog",
        "title": "Vaivai: View Error Log"
      },
      {
        "command": "vaivai.clearErrorLog",
        "title": "Vaivai: Clear Error Log"
      },
      {
        "command": "vaivai.initConfig",
        "title": "Vaivai: Initialize Configuration"
      }
    ],
    "keybindings": [
      {
        "command": "vaivai.generateFileList",
        "key": "ctrl+alt+f",
        "mac": "cmd+alt+f"
      },
      {
        "command": "vaivai.generateContentOverview",
        "key": "ctrl+alt+o",
        "mac": "cmd+alt+o"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^29.5.13",
    "@types/mocha": "^8.2.3",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}
